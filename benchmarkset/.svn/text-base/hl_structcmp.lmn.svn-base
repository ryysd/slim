{
  {
    start(5, 0).

    /* hyperlink cmp */    
    init @@ start(S, I) :- S > I, I1 = I+1, S1 = S-1, new($h) | start(S, I1), a($h), a($h), a($h), b($h), b($h), b($h).

  }.
  {$p, @p}/ :- $p.
  ab @@ a($x), b($x) :- hlink($x) | .
}.
